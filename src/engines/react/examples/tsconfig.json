{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "jsxFactory": "h",
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["*.tsx", "*.d.ts"]
}
